{
  "name": "@apollo-elements/eslint-config",
  "version": "1.1.4",
  "description": "Apollo Elements JavaScript Code Standards",
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "MIT",
  "main": "index.cjs.js",
  "module": "index.js",
  "keywords": [
    "eslint"
  ],
  "scripts": {
    "prepare": "npx rollup --input index.js --output.file index.cjs.js --output.format cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollo-elements/apollo-elements.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "engines": {
    "node": ">= 8.x",
    "npm": ">= 6.x"
  },
  "peerDependencies": {
    "eslint": ">= 5",
    "eslint-config-google": "^0",
    "eslint-plugin-html": "^5",
    "eslint-plugin-jsdoc": "^4",
    "eslint-plugin-json": "^1",
    "eslint-plugin-no-loops": "^0"
  }
}
